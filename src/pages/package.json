{
	"name": "mc-grove-place",
	"private": true,
	"version": "0.1.0",
	"description": "mc.grove.place status page",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "wrangler pages deploy .svelte-kit/cloudflare"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^6",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"wrangler": "^3"
	}
}
